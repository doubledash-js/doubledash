{
  "name": "doubledash",
  "version": "1.0.0",
  "description": "Double Dash is simple utility library meant to help transition people off of jQuery.",
  "main": "src/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "docs": "documentation build --markdown-toc=false dist src/animate/index.js -f md > doc.md",
    "css": "npm-run-all css-compile css-compress",
    "css-compile": "sass src/css/animate.scss dist/doubledash-animate.css",
    "css-compress": "sass dist/doubledash-animate.css dist/doubledash-animate.min.css --style compressed",
    "js": "webpack"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "jest-environment-node",
    "transform": {},
    "setupFiles": [
      "./test/config/mocks/dom.js"
    ]
  },
  "author": "Demetri Mihalakakos",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "babel-loader": "^8.2.2",
    "documentation": "^13.2.5",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "sass": "^1.34.0",
    "webpack": "^5.26.1",
    "webpack-cli": "^4.6.0"
  },
  "dependencies": {}
}
